TYPE POLYGON
CONNECTIONTYPE OGR
CONNECTION data/set.json
STATUS ON
SIZEUNITS pixels
CLASS
  NAME all_green
  STYLE
    WIDTH 5
    OUTLINECOLOR 255 255 0
    COLOR 0 255 0
  END
END
CLASS
  NAME red_3_5
  EXPRESSION ([N_EDGES] >= 3 and [N_EDGES] <= 5)
  STYLE
    WIDTH 5
    OUTLINECOLOR 255 255 0
    COLOR 255 0 0
  END
END
CLASS
  NAME blue_quadrilateral
  EXPRESSION ([N_EDGES] == 4)
  STYLE
    WIDTH 5
    OUTLINECOLOR 255 255 0
    COLOR 0 0 255
  END
END

